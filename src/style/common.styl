// 弹框表格
.burbox .el-dialog__body {
  padding: 0;
}
.tableDialog{
  display: flex;
  height: 550px;
  .tabs{
    width: 170px;
    border-right: 2px solid #f3f8fe;
    .nav-title{
      margin-top: 40px;
      background: #f7f7f7;
      line-height: 40px;
      text-indent: 15px;
      font-size: 17px;
      font-weight: bold;
      color: #000;
    }
    p{
      margin-top: 40px;
      background: #f7f7f7;
      line-height: 36px;
      text-indent: 20px;
      color:#20a1ff;
    }
    .el-menu {
      border-right: 0;
      .title{
        font-size: 15px;
        font-weight: bold;
      }
      .el-menu-item, .el-submenu__title {
        height: 45px;
        line-height: 45px;
      }
    }
  }
  .burli1{
    width: 400px;
    border-right: 2px solid #f3f8fe;
    padding: 40px 20px 0 20px;
  }
  .burli2{
    width: 400px;
    margin-bottom: 20px;
    padding: 40px 20px 0 20px;
  }
  .burli3{
    width: 500px;
    margin-bottom: 20px;
    padding: 40px 20px 0 20px;
  }
}


